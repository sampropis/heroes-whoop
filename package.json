{
  "name": "whoop-developer-api-test-site",
  "version": "1.0.0",
  "description": "Web app that utilizes the WHOOP Developer API with OAuth flow and cycle data display",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production npm run build && NODE_ENV=production node dist/app.js",
    "dev": "NODE_ENV=development nodemon --watch 'app.ts' --exec 'ts-node app.ts'",
    "locald": "NODE_ENV=locald nodemon --watch 'app.ts' --exec 'ts-node app.ts'",
    "localp": "NODE_ENV=localp npm run build && NODE_ENV=localp node dist/app.js",
    "test": "jest --coverage",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p tsconfig.json",
    "build:frontend": "tsc -p public/tsconfig.json"
  },
  "keywords": [
    "whoop",
    "api",
    "oauth",
    "health",
    "fitness"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.2.0",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
